{
    "language"        : "PYTHON",
    "experiment-name" : "branin-distributed-example",
    "polling-time"    : 1,
    "database" :{
	"address": "frontend.cluster"
 },
    "resources" : {
        "cluster" : {
            "scheduler"         : "SGE",
            "max-concurrent"    : 3,
            "max-finished-jobs" : 1000,
	    "environment-file" : "~/.conda/envs/spearmint/bin/activate spearmint"
        }
    },
    "tasks": {
        "branin" : {
            "type"       : "OBJECTIVE",
            "likelihood" : "NOISELESS",
            "main-file"  : "branin",
            "resources"  : "cluster"
        }
    },
    "variables": {
        "x" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : -5,
            "max"  : 10
        },
        "y" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : 0,
            "max"  : 15
        }
    }
}

